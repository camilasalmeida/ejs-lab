<%- include('./partials/nav') %>

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title> <%= category %> </title>
</head>

<body>
    <h1><%= category %></h1>

  <ul> 
    <% menuItems.forEach((item, index) => { %>
        <li>
        <h2><%= item.name %></h2>, <%= item.price %>, <%= item.rating %>, <%= item.details %>
        </li>
     <% }); %> 
      </ul>

</body>
</html>

